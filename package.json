  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon ./src/server.js --verbose",
    "deploy-commands": "node src/discord/deploy-commands.js",
